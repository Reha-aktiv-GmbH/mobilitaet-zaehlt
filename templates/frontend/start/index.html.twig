        }

        .paralsec1 {
            background-image: url("17833-_MG_2141.jpg");}

        .paralsec2 {
            background-image: url("ORION PRO BE19.jpg");
        }

        /* Add more images for more sections */

        /* Remove Bottom Margin from Jumbotron */
        .jumbotron{margin-bottom: 0;}

        /* Footer */
        .wn-footer {
            padding: 2.5rem 0;
            text-align: center;
            color: white;
            background: rgb(2,81,150);

        }

        .wn-footer a {
