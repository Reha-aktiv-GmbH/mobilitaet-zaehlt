        }

        #teaser {
            z-index: 2;
            position: absolute;
            top:3.2rem;
            right: 20rem;
        }

        #teaser-right {
            background: rgb(255,255,255);
            height: 78px;
            padding:.4rem 1.5rem .4rem 1.5rem;
            z-index: 1;
            position: absolute;
            top:0;
            right: 4.5rem;
            transform: skew(-40deg);
            min-width: 290px;
        }

        #teaser-right * {
            transform: skew(40deg);
        }
        #teaser-right img {
            padding:0 1.5rem 0 2.2rem;
        }

        #teaser-right::after {
            content: ' ';
            transform: skew(-40deg);
        }

        .navbar-nav {
            margin-left:9rem;
        }


        .bx-white::after {
            background: rgba(255,255,255,1) !important;
        }
        .bx-lightwhite::after {
            background: rgba(255,255,255,0.6) !important;
        }
        .bx-whiteblue::after {
            background: rgb(149,179,215) !important;
        }

        .bx-lightblue::after {
            background: rgb(79,129,189) !important;
        }
        .bx-darkblue::after {
            background: rgb(55,96,146) !important;
        }

        .bg-lightblue {
            background: rgb(149,179,215) !important;
        }

        .bg-lightblue * {
            color:rgb(55,96,146);
        }

        .bg-blue {
            background: rgb(79,129,189) !important;
        }

        .bg-blue *, .bg-darkblue * {
            color:rgb(255,255,255);
        }

        .bg-darkblue {
            background: rgb(55,96,146) !important;
        }

        .text-red {
            color:rgb(210,17,48);
        }

