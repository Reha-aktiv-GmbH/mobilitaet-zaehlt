        }
        /* GLOBAL STYLES
-------------------------------------------------- */
        /* Padding below the footer and lighter body text */

        /*body {
            padding-top: 3rem;
            padding-bottom: 3rem;
            color: #5a5a5a;
        }*/

        .bg-blue {
            background: rgb(2,81,150);
            color:white;
        }
        .bg-blue * {
            color:white;
        }


        .bx {
            display: inline-block;
            position: relative;
            padding: 1em 1.2rem;
            text-decoration: none;

        }
        .bx::after {
            content: '';
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            z-index: 1;
            transform: skew(-40deg);
            -webkit-box-shadow: 2px 3px 11px -3px rgba(0,0,0,0.77);
            -moz-box-shadow: 2px 3px 11px -3px rgba(0,0,0,0.77);
